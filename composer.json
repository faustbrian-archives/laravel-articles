{
    "name": "artisanry/articles",
    "description": "Easy Articles for Laravel",
    "keywords": ["laravel", "framework", "articles", "eloquent", "tags", "slug"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@basecode.sh",
        "homepage": "https://basecode.sh"
    }],
    "require": {
        "php": "^7.1.3",
        "illuminate/database": "5.6.* || 5.7.*",
        "illuminate/support": "5.6.* || 5.7.*",
        "spatie/laravel-model-status": "^1.5",
        "spatie/laravel-sluggable": "^2.1",
        "spatie/laravel-tags": "^2.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\Articles\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\Articles\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\Articles\\ArticlesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
